<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.MyOtherCatalog" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>

    <style name="Theme.MyOtherCatalog" parent="Base.Theme.MyOtherCatalog" />

    <style name="circleImageView" parent="">
        <item name ="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>


    <style name="CustomTheme" parent="Theme.AppCompat.Light">
        <item name="colorAccent">#3E1739</item>
        <item name="android:windowActivityTransitions">true</item>
        <item name="android:windowEnterTransition">@android:transition/slide_right</item>
        <item name="android:windowExitTransition">@android:transition/slide_left</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">#FCACFF</item>

    </style>
    
    <style name="CustomTitles" parent="TextAppearance.AppCompat">
        <item name="android:textSize">30dp</item>
        <item name="android:textStyle">italic</item>
    </style>


    





    
